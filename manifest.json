{
    "name": "Онлайн таймер",
    "description": "Онлайн таймер for 'Профессиональных Горизонтов'",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png",
        "512": "icons/icon512.png"
    },
    "action": {
        "default_title": "Кликни по мне, чтобы узнать оставшееся время :)",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "background",
        "webNavigation"
    ],
    "background": {}
}